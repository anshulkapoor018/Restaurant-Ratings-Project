<div class="card-center">
    <h3>Add Restaurant as Owner</h3>
    <form method="POST" action="/restaurants/add">
        <input type="text" name="name" placeholder="Restaurant Name" />
        <input type="text" name="website" placeholder="Restaurant Website" />
        <input type="text" name="category" placeholder="Restaurant Category" />
        <input type="text" name="address" placeholder="Restaurant Address" />
        <input type="text" name="city" placeholder="Restaurant City" />
        <input type="text" name="state" placeholder="Restaurant State" />
        <input type="text" name="zip" placeholder="Restaurant Zip Code" />
        <input type="text" name="longitude" placeholder="Restaurant Longitude" />
        <input type="text" name="latitude" placeholder="Restaurant Latitude" />
        <input type="submit" value="Add Restaurant" />
    </form>
</div>
<div class="card-center">
    <h3>Manage Owned Restaurants</h3>
    <p>(Click links to edit)</p>
    {{#each restaurants}}
        <p><a href="/restaurants/edit/{{this._id}}">{{this.name}}</p></a>
    {{/each}}
</div>